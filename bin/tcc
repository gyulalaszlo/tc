#!/usr/bin/env node

var program = require('commander');

program
  .version('0.0.1')
  .command('parse [packages]', 'Parse packages')
  .command('build [packages]', 'Build the output for packages')
  //.command('search [query]', 'search with optional query')
  //.command('list', 'list packages installed')
  .parse(process.argv);
